{
  "name": "myvitelib",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "myvitelib/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/myvitelib",
        "main": "myvitelib/src/index.ts",
        "tsConfig": "myvitelib/tsconfig.lib.json",
        "assets": [
          "myvitelib/*.md"
        ]
      }
    }
  },
  "tags": []
}
